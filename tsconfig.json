{
  "compilerOptions": { 
    "target": "es5",
    "module": "CommonJS",
    "outDir": "./dist",
	"strict": true,
	"moduleResolution": "node",
	"baseUrl": ".",
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
	"resolveJsonModule": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
